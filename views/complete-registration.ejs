<section class="bg-white p-8 rounded-lg shadow-md max-w-md w-full main-section">
  <h2 class="text-2xl font-semibold text-center mb-6">Complete Your Registration</h2>
  <form id= "comRegForm" action="/complete-registration" method="POST" class="space-y-4">
    <div>
      <label for="userName" class="block text-gray-700 mb-1">Name:</label>
      <input type="userName" id="userName" name="userName" class="p-2 border border-gray-300 rounded w-full" required>

      <label for="email" class="block text-gray-700 mb-1">Email:</label>
      <input type="email" id="email" name="email" class="p-2 border border-gray-300 rounded w-full" required>

      <label for="password" class="block text-gray-700 mb-2">New Password:</label>
      <input type="password" id="password" name="password" class="p-2 border border-gray-300 rounded">

    </div>
    <button type="submit" class="bg-blue-500 text-white w-full py-2 rounded hover:bg-blue-600 transition duration-200">Submit</button>
  </form>
</section>
<script src="/js/complete-registration.js"></script>